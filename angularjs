export function getEnumValue(value: string):string | undefined{
var str=undefined;
 Object.keys(ReportTypesEnum).forEach(function(key) {
  var val = ReportTypesEnum[key];
   if (key == value) str= val; 
  //console.log(key + " "+ val);   
}); 
 return str; 
}


